{
  "name": "react-contextmenu-lite",
  "version": "1.0.9",
  "description": "基于 React 实现的右键菜单，仅提供基本功能，样式由你自己定义",
  "main": "lib/index.js",
  "scripts": {
    "build": "webpack",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "pretty-quick": "pretty-quick"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rt-zhangxuefei/react-contextmenu.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "contextmenu",
    "rightmenu",
    "rightclick"
  ],
  "author": "rt-zhangxuefei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rt-zhangxuefei/react-contextmenu/issues"
  },
  "homepage": "https://github.com/rt-zhangxuefei/react-contextmenu#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
